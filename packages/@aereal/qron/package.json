{
  "author": "aereal <aereal@aereal.org>",
  "license": "MIT",
  "main": "built/index.js",
  "types": "built/index.d.ts",
  "name": "@aereal/qron",
  "repository": "git@github.com:aereal/qron.git",
  "version": "0.0.4-alpha.0",
  "scripts": {
    "test": "jest",
    "build": "rm -rf built/ && tsc -p tsconfig.build.json",
    "docs": "rm -rf docs/ && typedoc --options typedoc.json src/"
  },
  "peerDependencies": {
    "@aws-cdk/aws-dynamodb": ">= 1.66.0",
    "@aws-cdk/aws-events": ">= 1.66.0",
    "@aws-cdk/aws-iam": ">= 1.66.0",
    "@aws-cdk/aws-stepfunctions": ">= 1.66.0",
    "@aws-cdk/aws-stepfunctions-tasks": ">= 1.66.0",
    "@aws-cdk/core": ">= 1.66.0"
  },
  "devDependencies": {
    "@aereal/eslint-config": "^0.1.4",
    "@aws-cdk/assert": "^1.66.0",
    "@aws-cdk/aws-dynamodb": "^1.66.0",
    "@aws-cdk/aws-events": "^1.66.0",
    "@aws-cdk/aws-events-targets": "^1.66.0",
    "@aws-cdk/aws-iam": "^1.66.0",
    "@aws-cdk/aws-sns": "^1.66.0",
    "@aws-cdk/aws-stepfunctions": "^1.66.0",
    "@aws-cdk/aws-stepfunctions-tasks": "^1.66.0",
    "@aws-cdk/core": "^1.66.0",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "constructs": "^3.0.4",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "semantic-release": "^17.1.2",
    "ts-jest": "^26.4.1",
    "typedoc": "^0.17.0-3",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": [
      "@aereal/eslint-config/presets/docs-typescript"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "createDefaultProgram": true,
      "project": "./tsconfig.json"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aereal/cdk-dynamodb-expression": "^1.0.0"
  },
  "release": {
    "branches": [
      {
        "name": "main",
        "prerelease": true
      },
      "release"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
