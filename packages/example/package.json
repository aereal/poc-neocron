    "@aws-cdk/aws-autoscaling-hooktargets": "^1.20.0",
    "@aws-cdk/aws-certificatemanager": "^1.20.0",
    "@aws-cdk/aws-cloudformation": "^1.20.0",
    "@aws-cdk/aws-cloudfront": "^1.20.0",
    "@aws-cdk/aws-cloudwatch": "^1.20.0",
    "@aws-cdk/aws-codebuild": "^1.20.0",
    "@aws-cdk/aws-codecommit": "^1.20.0",
    "@aws-cdk/aws-codepipeline": "^1.20.0",
    "@aws-cdk/aws-dynamodb": "^1.20.0",
    "@aws-cdk/aws-ec2": "^1.20.0",
    "@aws-cdk/aws-ecr": "^1.20.0",
    "@aws-cdk/aws-ecr-assets": "^1.20.0",
    "@aws-cdk/aws-ecs": "^1.20.0",
    "@aws-cdk/aws-elasticloadbalancing": "^1.20.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.20.0",
    "@aws-cdk/aws-events": "^1.20.0",
    "@aws-cdk/aws-events-targets": "^1.20.0",
    "@aws-cdk/aws-iam": "^1.20.0",
    "@aws-cdk/aws-kms": "^1.20.0",
    "@aws-cdk/aws-lambda": "^1.20.0",
    "@aws-cdk/aws-logs": "^1.20.0",
    "@aws-cdk/aws-route53": "^1.20.0",
    "@aws-cdk/aws-route53-targets": "^1.20.0",
    "@aws-cdk/aws-s3": "^1.20.0",
    "@aws-cdk/aws-s3-assets": "^1.20.0",
    "@aws-cdk/aws-sam": "^1.20.0",
    "@aws-cdk/aws-secretsmanager": "^1.20.0",
    "@aws-cdk/aws-servicediscovery": "^1.20.0",
    "@aws-cdk/aws-sns": "^1.20.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.20.0",
    "@aws-cdk/aws-sqs": "^1.20.0",
    "@aws-cdk/aws-ssm": "^1.20.0",
